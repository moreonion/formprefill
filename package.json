{
  "type": "module",
  "name": "@moreonion/jquery.formprefill",
  "version": "0.13.0",
  "description": "Form prefill plugin for jQuery",
  "source": "src/main.js",
  "main": "dist/jquery.formprefill.cjs",
  "module": "dist/jquery.formprefill.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "microbundle watch src/main.js",
    "dev-build": "microbundle -i src/main.js --no-compress && cp dist/jquery.formprefill.cjs jquery.formprefill.js",
    "dist": "microbundle -i src/main.js && cp dist/jquery.formprefill.cjs jquery.formprefill.min.js",
    "lint": "eslint src",
    "fix": "eslint --fix src/**/*.js"
  },
  "keywords": [
    "jquery-plugin"
  ],
  "author": "",
  "license": "LGPL-3.0",
  "devDependencies": {
    "core-js": "3",
    "microbundle": "^0.15.1"
  },
  "dependencies": {},
  "homepage": "https://github.com/moreonion/jquery.formprefill#readme",
  "publishConfig": {
    "access": "public"
  }
}
